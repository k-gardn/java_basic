<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxWidth="190.0" prefHeight="343.0" prefWidth="190.0" spacing="15.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox prefHeight="43.0" prefWidth="300.0" spacing="15.0">
         <children>
            <Label text="ID">
               <padding>
                  <Insets right="5.0" top="5.0" />
               </padding>
            </Label>
            <TextField fx:id="id" />
         </children>
      </HBox>
      <HBox prefHeight="51.0" prefWidth="300.0" spacing="15.0">
         <children>
            <Label text="PW">
               <padding>
                  <Insets top="5.0" />
               </padding>
            </Label>
            <TextField fx:id="pw" prefWidth="138.0" />
         </children>
      </HBox>
      <HBox prefHeight="40.0" prefWidth="300.0" spacing="15.0">
         <children>
            <RadioButton fx:id="w" mnemonicParsing="false" text="여">
               <toggleGroup>
                  <ToggleGroup fx:id="genderGroup" />
               </toggleGroup>
            </RadioButton>
            <RadioButton fx:id="m" mnemonicParsing="false" text="남" toggleGroup="$genderGroup" />
         </children>
      </HBox>
      <HBox prefHeight="40.0" prefWidth="300.0" spacing="15.0">
         <children>
            <CheckBox fx:id="game" mnemonicParsing="false" text="게임" />
            <CheckBox fx:id="study" mnemonicParsing="false" text="공부" />
         </children>
      </HBox>
      <HBox prefHeight="36.0" prefWidth="267.0" spacing="15.0">
         <children>
            <Label text="하고 싶은 말" />
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <TextArea fx:id="say" prefHeight="50.0" prefWidth="170.0" />
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="reg" mnemonicParsing="false" prefHeight="75.0" prefWidth="170.0" text="회원가입" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
